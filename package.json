{
  "name": "puppy-redux-observable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"REACT_APP_URL=http://localhost:4000 yarn start\"",
    "eject": "react-scripts eject",
    "server": "json-server --watch db.json --port 4000",
    "sort": "sort-package-json",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,tsx,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "normalizr": "3.4.1",
    "puppy-components": "1.1.2",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.1",
    "react-scripts": "3.1.2",
    "react-spring": "8.0.27",
    "redux": "4.0.4",
    "redux-observable": "1.2.0",
    "redux-saga": "1.1.1",
    "reselect": "4.0.0",
    "rxjs": "6.5.3",
    "styled-components": "4.4.0",
    "with-http-reducer": "^1.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "concurrently": "4.1.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "husky": "3.0.5",
    "json-server": "0.15.1",
    "lint-staged": "9.4.0",
    "prettier": "1.18.2",
    "redux-devtools-extension": "2.13.8",
    "redux-immutable-state-invariant": "2.1.0",
    "sort-package-json": "1.22.1"
  },
  "engines": {
    "node": "12.10.0",
    "yarn": "1.17.3"
  }
}
